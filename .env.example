# Deepgram API (Speech-to-Text)
# Get your API key from: https://console.deepgram.com/
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# ElevenLabs API (Text-to-Speech) - Optional
# Get your API key from: https://elevenlabs.io/
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# OpenRouter API (AI Brain - GPT-4o, Claude, etc.)
# Get your API key from: https://openrouter.ai/
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Hume AI API (Emotion Detection)
# Get your API keys from: https://platform.hume.ai/
HUME_API_KEY=your_hume_api_key_here
HUME_SECRET_KEY=your_hume_secret_key_here

# Portal Configuration
PORT=3001
USE_DEEPGRAM_FLUX=true

